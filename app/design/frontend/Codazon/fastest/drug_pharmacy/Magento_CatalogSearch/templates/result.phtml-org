<div class="search results">
    <?php if ($messages = $block->getNoteMessages()) : ?>
    <div class="message notice">
        <div>
            <?php foreach ($messages as $message) : ?>
                <?= /* @noEscape */ $message ?><br />
            <?php endforeach; ?>
        </div>
    </div>
    <?php endif; ?>
    <?= /* @noEscape */ $productList ?>
</div>
<?php else : ?>

<div class="message notice">
    <div>
        <?php echo "No matching records found... Don't give up! We are here to help you get what you need! <a onclick='Tawk_API.toggle()' href='javascript::void(0)'>Chat</a> with one of our procurement specialists now and let us know what you're looking for." ?>
