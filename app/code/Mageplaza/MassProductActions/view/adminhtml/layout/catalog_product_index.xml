<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Mageplaza_MassProductActions::css/style.css"/>
        <css src="Mageplaza_Core::css/grid-mageplaza.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\MassProductActions\Block\Adminhtml\Result" name="mpmassproductactions_form_before" template="Mageplaza_MassProductActions::form/before.phtml" before="-"/>
            <block class="Magento\Backend\Block\Template" name="mpmassproductactions_attribute_set_popup" template="Mageplaza_MassProductActions::form/popup/attribute-set.phtml" before="-">
                <block class="Mageplaza\MassProductActions\Block\Adminhtml\AttributeSet\Edit" name="mpmassproductactions_attribute_set_form" as="attribute_set_form"/>
            </block>
            <block class="Magento\Backend\Block\Template" name="mpmassproductactions_category_popup" template="Mageplaza_MassProductActions::form/popup/category.phtml" before="-">
                <block class="Mageplaza\MassProductActions\Block\Adminhtml\Category\Edit" name="mpmassproductactions_category_form" as="category_form"/>
            </block>
            <block class="Magento\Backend\Block\Template" name="mpmassproductactions_website_popup" template="Mageplaza_MassProductActions::form/popup/website.phtml" before="-">
