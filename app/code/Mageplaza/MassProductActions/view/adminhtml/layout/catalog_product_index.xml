            <block class="Magento\Backend\Block\Template" name="mpmassproductactions_attribute_popup" template="Mageplaza_MassProductActions::form/popup/attribute.phtml" before="-"/>
            <block class="Magento\Backend\Block\Template" name="mpmassproductactions_inventory_popup" template="Mageplaza_MassProductActions::form/popup/inventory.phtml" before="-"/>
